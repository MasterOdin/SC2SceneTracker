{
  "name": "SC2 Scene Tracker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Keeping track of twitch SC2 streams for you!",
  "icons": {
       "16": "images/icon16.png",
       "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "browser_action": {
      "default_icon": "images/icon16.png",
      "default_title": "SC2 Stream Tracker",
      "default_popup": "popup.html"
  },
  "permissions": [ 
      "https://api.twitch.tv/",
      "http://day9.tv/",
      "http://www.gosugamers.net/",
      "http://sc2.mpevelerapi.com/",
      "tabs"
    ]
}